#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/cli/bin/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/cli/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/cli/bin/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/cli/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules/@babel/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/@babel+cli@7.18.10_@babel+core@7.18.10/node_modules:/mnt/d/proj_web/A_openSource/backend/fast-gateway-pnpm-3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/cli/bin/babel.js" "$@"
else
  exec node  "$basedir/../@babel/cli/bin/babel.js" "$@"
fi
